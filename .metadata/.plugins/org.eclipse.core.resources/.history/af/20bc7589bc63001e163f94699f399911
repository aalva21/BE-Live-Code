package com.promineotech;
import java.util.*;
import java.io.*;

public class Application {

	public static void main(String[] args) {
		//System.out.println("My first Java Program!"); // Will print out message 
		
	// ---------------------------------------------------------------------------------------------------------------------- \\
	int[] arr = {-11, 43, 901, 43, -5};

	arr[arr.length - 2] = 0;

	for (int n : arr){
	    System.out.print(n + " ");
	}
	}
}